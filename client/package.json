{
  "name": "client",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "es6-promise": "^4.1.0",
    "isomorphic-fetch": "^2.2.1",
    "node-sass": "^4.5.2",
    "prop-types": "^15.5.8",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "react-router-dom": "^4.0.0",
    "react-scripts": "0.9.5"
  },
  "proxy": "http://localhost:3001",
  "scripts": {
    "watch-sass": "npm run build-sass && node-sass --watch --recursive --source-map true --source-map-contents --output src src",
    "build-sass": "node-sass --recursive --output src src",
    "start": "react-scripts start",
    "build": "npm run build-sass && react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  }
}
